\n  computed: {\n    fixedDate () {\n      const rawDate = new Date(this.date)\n      const fixedDate = new Date(\n        rawDate.getTime() \n        + rawDate.getTimezoneOffset() \n        * 60000\n      ).toISOString()\n\n      return fixedDate\n    },\n    niceDate () {\n      const newDate = dayjs(this.fixedDate).format(this.format)\n\n      return newDate\n    }\n  }\n}\n</script>"],"sourceRoot":"docs/.vuepress/theme/global-components"}]}